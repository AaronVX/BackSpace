<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.group8.backspace">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".BookBrowseActivity"></activity>
        <activity android:name=".BookDestinationActivity" />
        <activity android:name=".PurchaseTicket" />
        <activity
            android:name=".Listview"
            android:label="@string/title_activity_listview"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FlightStatus"
            android:label="@string/title_activity_flight_status"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TravelClass"
            android:label="@string/title_activity_travel_class"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".BookOriginActivity"
            android:label="@string/title_activity_book"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FlightDetailActivity"
            android:label="@string/title_activity_flight_detail" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>